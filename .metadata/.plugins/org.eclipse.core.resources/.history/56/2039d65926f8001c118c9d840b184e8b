import pymysql

conn = pymysql.connect(host='localhost', user='root', password='python', db='encore', charset='utf8')

cur = conn.cursor()

sql = 'select * from emp'
vals = ("e_id","e_name","sex","addr")
cur.execute(sql,vals)

for row in cur:
        print(row[0], row[1], row[2], row[3])
        

conn.close()